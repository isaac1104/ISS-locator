  },
  "homepage": "https://github.com/isaac1104/ISS-locator#readme",
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.18.2",
    "helmet": "^3.21.0"
  }
